<p id="notice"><%= notice %></p>

<h1>Listing Course Photos</h1>

<table>
  <thead>
    <tr>
      <th>Photo</th>
      <th>Course</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @course_photos.each do |course_photo| %>
      <tr>
        <td><%= course_photo.photo %></td>
        <td><%= course_photo.course %></td>
        <td><%= link_to 'Show', course_photo %></td>
        <td><%= link_to 'Edit', edit_course_photo_path(course_photo) %></td>
        <td><%= link_to 'Destroy', course_photo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Course photo', new_course_photo_path %>
